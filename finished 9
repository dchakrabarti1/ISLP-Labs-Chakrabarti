        "def abline(ax, m, b,*args,**kwargs):\n",
        "  xlim = ax.get_xlim()\n",
        "  ylim = [m*xlim[0] +b , m*xlim[1]+b]\n",
        "  ax.plot(xlim,ylim,*args,**kwargs)\n",
        "\n",
